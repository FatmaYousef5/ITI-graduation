@model MansionSliderViewModel
@{
    ViewData["Title"] = "Select Category";
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/SliderStyle.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap/5.1.3/js/bootstrap.min.js"
            integrity="sha512-kgBIOG9s9ogF/IK+L8QvTfM60U6jjJTD6nKl6"></script>

    <title>Slider</title>
</head>

<body>

<section>
    <nav class="container navbar">
        <div class="container-fluid">
            <a class="navbar-brand" asp-action="Index" asp-controller="Home">
                <img class="photo" src="~/Images/Airbnb_Logo_Bélo.svg.png" alt="logo" class="img-fluid">
            </a>
            <a asp-action="Index" asp-controller="Home" class="btn btn-outline-dark rounded-pill">Quit</a>

        </div>
    </nav>
</section>
<form enctype="multipart/form-data" method="post" asp-action="GenerateMansion">
<div class="cs-slider">
        <div class="cs-slider-wrapper">

            <div class="container m-auto ">
                <div class="row m-5">
                    <div class="col-4 ms-5 mt-5 pt-5 ">
                        <span class="fw-bolder">
                            Step 1
                        </span> <br>
                        <h2>
                            Tell us about your place
                        </h2>
                        <p>
                            In this step, we'll ask you which type of property you have and if guests will book the
                            entire place or just a room. Then let us know the location and how many guests can stay.
                        </p>

                    </div>

                    <div class="col-4 ms-5 mt-5  ">
                        <!-- <video
                            class="w-100  h-50" src="/Step 1- Tell us about your place.mp4"
                            autoplay>
                            </video> -->

                        <video class="firstv w-100 h-100" autoplay>
                            <source src="~/Images/Slider/Step 1- Tell us about your place.mp4" type="video/mp4" />
                        </video>
                    </div>
                </div>
            </div>


        </div>


        <div class="cs-slider-wrapper cs-hide">
            <div class="container">
                <div class="container mt-5">
                    <h1 class="text-center">Which of these best describes your place?</h1>
@*                    <!-- <p class="text-center">You'll need 5 photos to get started. You can add more or make changes later. -->
                    </p>*@
                </div>
                <div class="row justify-content-center">
                    @foreach (var mansionCategory in @Model.MansionCategories)
                    {
                        <div class="col-md-2">
                            <label>
                                <input id="input" class="card-radio-input" type="radio"
                                       name="placeType" value="@mansionCategory.Id" required/>
                                <div id="category" class="card">
                                    <img id="icon" class="card-img-top"
                                         src="https://a0.muscache.com/pictures/4d7580e1-4ab2-4d26-a3d6-97f9555ba8f9.jpg"
                                         alt="Entire place photo" />
                                    <div id="body" class="card-body">
                                        <h5 id="title" class="card-title">@mansionCategory.Name</h5>
                                        <p class="card-text"></p>
                                    </div>
                                </div>
                            </label>
                        </div>
                    }
                </div>
            </div>

        </div>

        <div class="cs-slider-wrapper cs-hide">

            <section>
                <div class="mt-5 d-flex justify-content-center">
                    <div class=" d-flex flex-column ">
                        <h1>Mansion address</h1>
                        <p class="text-secondary">
                            Your address is only shared with guests after they’ve made a
                            reservation.
                        </p>
                        <div class="input-group has-validation ">
                            <span class="input-group-text" id="inputGroup-sizing-default">Street</span>
                            <input name="Street" type="text" class="form-control" aria-label="Sizing example input"
                                   aria-describedby="inputGroup-sizing-default" required>
                            <span asp-validation-for="@Model.Mansion.Street"></span>

                        </div>


                        <div class="input-group ">

                            <span class="input-group-text" id="inputGroup-sizing-default">City</span>
                            <input name="City" type="text" class="form-control" aria-label="Sizing example input"
                                       aria-describedby="inputGroup-sizing-default" id="validationCustom03" required>
                            <span asp-validation-for="@Model.Mansion.City"></span>
                                <div class="invalid-feedback">
                                    Please provide a valid city.
                                </div>

                        </div>

                        <div class="input-group ">
                            <span class="input-group-text" id="inputGroup-sizing-default">Zip Code </span>
                            <input name="Zipcode" type="text" class="form-control" aria-label="Sizing example input"
                                       aria-describedby="inputGroup-sizing-default" required>
                            <span asp-validation-for="@Model.Mansion.Zipcode"></span>
                        </div>

                        <div class="input-group ">
                            <span class="input-group-text" id="inputGroup-sizing-default">Build Number</span>
                                <input name="Build_No" type="text" class="form-control" aria-label="Sizing example input"
                                       aria-describedby="inputGroup-sizing-default" required>
                            <span asp-validation-for="@Model.Mansion.Build_No"></span>
                        </div>
                    </div>
                </div>

            </section>

        </div>



        <div class="cs-slider-wrapper cs-hide">

            <div class="d-flex flex-column p-lg-5 margin-edit">
                <div class="p-2 mb-3 text-start">
                    <h1>Share some basics about your place</h1>
                    <p class="fw-semibold text-muted">
                        You'll add more details later, like bed types.
                    </p>
                </div>
                <div class="d-grid gap-2 m-1 h4 pb-2 mb-4 border-bottom border-dark-subtle">
                    <div class="d-flex mb-3">
                            <input name="NoGuest" type="hidden" value="0" class="hidden-Guests-input" />
                        <div class="me-auto p-2">Guests</div>
                        <div class="p-2">
                            <button id="btn1" type="button" class="btn btn-light border border-dark-subtle rounded-5"
                                    onclick="Counertfunc()">
                                <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none"
                                     xmlns="http://www.w3.org/2000/svg">
                                    <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                    <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                                    <g id="SVGRepo_iconCarrier">
                                        <path d="M5 12H19" stroke="#323232" stroke-width="2" stroke-linecap="round"
                                              stroke-linejoin="round"></path>
                                        <path d="M12 5L12 19" stroke="#323232" stroke-width="2" stroke-linecap="round"
                                              stroke-linejoin="round"></path>
                                    </g>
                                </svg>
                            </button>
                        </div>
                        <div class="p-2 d-flex"><span class="p-2 counter">0</span></div>
                        <div class="p-2">
                            <button id="btn2" type="button" class="btn btn-light border border-dark-subtle rounded-5"
                                    onclick="CounertfuncDown()">
                                <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none"
                                     xmlns="http://www.w3.org/2000/svg">
                                    <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                    <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                                    <g id="SVGRepo_iconCarrier">
                                        <path d="M5 12H19" stroke="#323232" stroke-width="2" stroke-linecap="round"
                                              stroke-linejoin="round"></path>
                                    </g>
                                </svg>
                            </button>
                        </div>
                    </div>

                </div>
                <div class="d-grid gap-2 m-1 h4 pb-2 mb-4 border-bottom border-dark-subtle">

                    <div class="d-flex mb-3">
                        <input name="NoBedroom" type="hidden" value="0" class="hidden-Bedroom-input" />
                        <div class="me-auto p-2">Bedrooms</div>
                        <div class="p-2">
                            <button id="btn3" type="button" class="btn btn-light border border-dark-subtle rounded-5"
                                    onclick="Counertfunc1()">
                                <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none"
                                     xmlns="http://www.w3.org/2000/svg">
                                    <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                    <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                                    <g id="SVGRepo_iconCarrier">
                                        <path d="M5 12H19" stroke="#323232" stroke-width="2" stroke-linecap="round"
                                              stroke-linejoin="round"></path>
                                        <path d="M12 5L12 19" stroke="#323232" stroke-width="2" stroke-linecap="round"
                                              stroke-linejoin="round"></path>
                                    </g>
                                </svg>
                            </button>
                        </div>
                        <div class="p-2 d-flex"><span class="p-2 counter">0</span></div>
                        <div class="p-2">
                            <button id="btn4" type="button" class="btn btn-light border border-dark-subtle rounded-5"
                                    onclick="CounertfuncDown1()">
                                <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none"
                                     xmlns="http://www.w3.org/2000/svg">
                                    <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                    <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                                    <g id="SVGRepo_iconCarrier">
                                        <path d="M5 12H19" stroke="#323232" stroke-width="2" stroke-linecap="round"
                                              stroke-linejoin="round"></path>
                                    </g>
                                </svg>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="d-grid gap-2 m-1 h4 pb-2 mb-4 border-bottom border-dark-subtle">
                    <div class="d-flex mb-3">
                            <input name="NoBed" type="hidden" value="0" class="hidden-Bed-input" />
                        <div class="me-auto p-2">Beds</div>
                        <div class="p-2">
                            <button id="btn5" type="button" class="btn btn-light border border-dark-subtle rounded-5"
                                    onclick="Counertfunc2()">
                                <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none"
                                     xmlns="http://www.w3.org/2000/svg">
                                    <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                    <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                                    <g id="SVGRepo_iconCarrier">
                                        <path d="M5 12H19" stroke="#323232" stroke-width="2" stroke-linecap="round"
                                              stroke-linejoin="round"></path>
                                        <path d="M12 5L12 19" stroke="#323232" stroke-width="2" stroke-linecap="round"
                                              stroke-linejoin="round"></path>
                                    </g>
                                </svg>
                            </button>
                        </div>
                        <div class="p-2 d-flex"><span class="p-2 counter">0</span></div>
                        <div class="p-2">
                            <button id="btn6" type="button" class="btn btn-light border border-dark-subtle rounded-5"
                                    onclick="CounertfuncDown2()">
                                <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none"
                                     xmlns="http://www.w3.org/2000/svg">
                                    <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                    <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                                    <g id="SVGRepo_iconCarrier">
                                        <path d="M5 12H19" stroke="#323232" stroke-width="2" stroke-linecap="round"
                                              stroke-linejoin="round"></path>
                                    </g>
                                </svg>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="d-grid gap-2 m-1 h4 pb-2 mb-4 border-bottom border-dark-subtle">
                    <div class="d-flex mb-3">
                            <input name="NoBathroom" type="hidden" value="0" class="hidden-Bathroom-input" />
                        <div class="me-auto p-2">Bathrooms</div>
                        <div class="p-2">
                            <button id="btn7" type="button" class="btn btn-light border border-dark-subtle rounded-5"
                                    onclick="Counertfunc3()">
                                <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none"
                                     xmlns="http://www.w3.org/2000/svg">
                                    <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                    <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                                    <g id="SVGRepo_iconCarrier">
                                        <path d="M5 12H19" stroke="#323232" stroke-width="2" stroke-linecap="round"
                                              stroke-linejoin="round"></path>
                                        <path d="M12 5L12 19" stroke="#323232" stroke-width="2" stroke-linecap="round"
                                              stroke-linejoin="round"></path>
                                    </g>
                                </svg>
                            </button>
                        </div>
                        <div class="p-2 d-flex"><span class="p-2 counter">0</span></div>
                        <div class="p-2">
                            <button id="btn8" type="button" class="btn btn-light border border-dark-subtle rounded-5"
                                    onclick="CounertfuncDown3()">
                                <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none"
                                     xmlns="http://www.w3.org/2000/svg">
                                    <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                    <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                                    <g id="SVGRepo_iconCarrier">
                                        <path d="M5 12H19" stroke="#323232" stroke-width="2" stroke-linecap="round"
                                              stroke-linejoin="round"></path>
                                    </g>
                                </svg>
                            </button>
                        </div>
                    </div>
                </div>
            </div>

        </div>

        <div class="cs-slider-wrapper cs-hide">

            <div class="container m-auto ">
                <div class="row m-5">
                    <div class="col-4 ms-5 mt-5 pt-5 ">
                        <span class="fw-bolder">
                            Step 2
                        </span> <br>
                        <h2>
                            Make your place stand out
                        </h2>
                        <p>
                            In this step, you’ll add some of the amenities your place offers, plus 5 or more photos. Then, you’ll create a title and description.

                        </p>

                    </div>

                    <div class="col-4 ms-5 mt-5 ">
                        <!-- <video
                        class="w-100  h-50" src="/Step 1- Tell us about your place.mp4"
                        autoplay>
                        </video> -->

                        <video class="firstv w-100 h-100" autoplay>
                            <source src="~/Images/Slider/Step 1- Tell us about your place.mp4" type="video/mp4" />
                        </video>
                    </div>
                </div>
            </div>

        </div>

        <div class="cs-slider-wrapper cs-hide">
            <div class="container">
              <div class="container">
                <h1 class="text-center">Tell guests what your place has to offer</h1>
                <p class="text-center">You can add more amenities after you publish your listing.</p>
              </div>
            <div class="container">
                <h5 class="text-center mb-3">Do you have any standout amenities?</h5>
              </div>
                <div class="row justify-content-center">
                    @foreach (var mansionServiceType in @Model.ServiceTypes)
                    {
                        <div class="text-center">

                            <h2>@mansionServiceType.ServiceType</h2>
                        </div>
                        @foreach (var mansionService in mansionServiceType.Service)
                        {
                        <div class="col-md-2 ">
                            <label class="d-flex justify-content-center">
                            <input id="input" class="card-radio-input" required type="checkbox" name="mansionService" value="@mansionService.Id" />
                              <div id="category" class="card">
                                <div id="body" class="card-body">
                                    <div>
                                         <h5 id="title" class="card-title">@mansionService.Name</h5>
                                                @*    <img id="icon" class="card-img-top"
                                                 src="https://a0.muscache.com/pictures/4d7580e1-4ab2-4d26-a3d6-97f9555ba8f9.jpg"
                                                 alt="Entire place photo" />*@
                                                    <svg width="20px" height="20px"
                                                 xmlns="http://www.w3.org/2000/svg" viewBox="0 0 640 512">
                                                        <path d="M64 64V352H576V64H64zM0 64C0 28.7 28.7 0 64 0H576c35.3 0 64 28.7 64 64V352c0 35.3-28.7 64-64 64H64c-35.3 0-64-28.7-64-64V64zM128 448H512c17.7 0 32 14.3 32 32s-14.3 32-32 32H128c-17.7 0-32-14.3-32-32s14.3-32 32-32z" />
                                                    </svg>
                                    </div>
@*                                  <p class="card-text">@mansionService.Description</p>

*@                              </div>
                              </div>
                            </label>
                          </div>
                        }
                    }
                </div>
            </div>

        </div>

        <div class="cs-slider-wrapper cs-hide">
            <div>
                <div class="container mt-5">
                    <h1 class="text-center">Add some photos of your house</h1>
                    <p class="text-center">
                        You'll need 5 photos to get started. You can add more or make changes later.
                    </p>
                </div>
                <div class="row justify-content-center">
                    <div class="col-md-8">
                        <div>
                            <!-- <h2 id="upload-status" class="card-header"> Drag your photos here </h2> -->
                            <div class="container">
                                <!-- <div id="upload-status" class="text-center">Choose at least 5 photos</div> -->
                                <div class="container img-fit-slider">
                                        <div class="file-input-container">
                                            <label for="file-input " class="upload-box drop">
                                                <span id="upload-status" class="drop-title"> Click to Upload photos
                                                    <br> or Drag your Photos here
                                                </span>
                                                <p> Choose at least 5 photos</p>
                                                <!-- <i class="material-icons" style="font-size:48px;color:rgb(126, 104, 104)">cloud_upload</i> -->
                                                <input type="file" id="fileInput" accept="image/*" name="photos"
                                                multiple required style="display: none;">
                                                <div id="image-container"></div>
                                            </label>
                                        </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>

        <div class="cs-slider-wrapper cs-hide">

            <div class="container m-auto d-flex justify-content-center ">
                <div class="row m-5">
                    <div>
                        <h1>
                            Now, let's give your apartment a title
                        </h1>
                        <span>
                            Short titles work best. Have fun with it—you can always change it later.
                        </span>
                        <br>

                        <div>

                            <div class="container mt-5">
                                <div class="row">
                                    <div class="col-md-12 d-flex justify-content-center">
                                            <textarea name="Title" required id="summary" onkeyup="validate()" class="form-control textarea"
                                                  placeholder="Type Here..."></textarea>
                                    </div>
                                </div>
                                <div class="d-flex justify-content-end">
                                    <span id="words_count" class="d-none">
                                        Words count - <span id="textcount"></span>
                                    </span>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>

        </div>

        <div class="cs-slider-wrapper cs-hide">

            <div class="container m-auto d-flex justify-content-center ">
                <div class="row m-5">
                    <div>
                        <h1>
                            Create your description
                        </h1>
                        <span>
                            Share what makes your place special.
                        </span>
                        <br>

                        <div>

                            <div class="container mt-5">
                                <div class="row">
                                    <div class="col-md-12 d-flex justify-content-center">
                                        <textarea required name="Description" id="desc" onkeyup="valid()" class="form-control textarea"
                                                  placeholder="Type Here..."></textarea>
                                    </div>
                                </div>
                                <div class="d-flex justify-content-end">
                                    <span id="words" class="d-none">Words count - <span id="text"></span></span>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>

        </div>

        <div class="cs-slider-wrapper cs-hide">


            <div class="container m-auto ">
                <div class="row m-5">
                    <div class="col-4 ms-5 mt-5 pt-5 ">
                        <span class="fw-bolder">
                            Step 3
                        </span> <br>
                        <h2>
                            Finish up and publish
                        </h2>
                        <p>
                            Finally, you’ll choose if you'd like to start with an experienced guest, then you'll set your
                            nightly price. Answer a few quick questions and publish when you're ready.

                        </p>

                    </div>

                    <div class="col-4 ms-5 mt-5  ">
                        <!-- <video
                            class="w-100  h-50" src="/Step 1- Tell us about your place.mp4"
                            autoplay>
                            </video> -->

                        <video class="firstv w-100 h-100" autoplay>
                            <source src="~/Images/Slider/Step 1- Tell us about your place.mp4" type="video/mp4" />
                        </video>
                    </div>
                </div>
            </div>
        </div>

        <div class="cs-slider-wrapper cs-hide">


            <section class="ms-5 mt-3">
                <div class="container d-flex justify-content-center ms-1 row-cols-3 ">
                    <div>
                        <div>
                            <h1>Now, set your price</h1>
                        </div>
                        <span class="text-secondary">You can change it anytime.</span><br>
                        <div class="bg-secondary-subtle rounded-4 secondcont  h-50 mt-4">

                            <div class="pt-2 d-flex justify-content-evenly ">


                                <div class="pt-2">
                                    <button id="firstbtn" type="button"
                                            class="btn btn-light border border-dark-subtle rounded-5" onclick="Increment()">
                                        <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none"
                                             xmlns="http://www.w3.org/2000/svg">
                                            <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                            <g id="SVGRepo_tracerCarrier" stroke-linecap="round"
                                               stroke-linejoin="round"></g>
                                            <g id="SVGRepo_iconCarrier">
                                                <path d="M5 12H19" stroke="#323232" stroke-width="2"
                                                      stroke-linecap="round" stroke-linejoin="round"></path>
                                                <path d="M12 5L12 19" stroke="#323232" stroke-width="2"
                                                      stroke-linecap="round" stroke-linejoin="round"></path>
                                            </g>
                                        </svg>
                                    </button>
                                </div>
                                <div class="p-2 d-flex justify-content-center bg-white p-2 rounded-1 w-50 ">
                                        <input name="Price" type="hidden" class="price-input" value="10" />
                                    <span class="p-2 c fw-bold text-center">$10</span>
                                </div>
                                <div class="p-2">
                                    <button id="secondbtn" type="button"
                                            class="btn btn-light border border-dark-subtle rounded-5" onclick="Decrement()">
                                        <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none"
                                             xmlns="http://www.w3.org/2000/svg">
                                            <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                            <g id="SVGRepo_tracerCarrier" stroke-linecap="round"
                                               stroke-linejoin="round"></g>
                                            <g id="SVGRepo_iconCarrier">
                                                <path d="M5 12H19" stroke="#323232" stroke-width="2"
                                                      stroke-linecap="round" stroke-linejoin="round"></path>
                                            </g>
                                        </svg>
                                    </button>
                                </div>
                            </div>

                            <div class="d-flex justify-content-center mt-2">Per Night</div>
                            <br>
                            <div class="d-flex justify-content-center mt-3">
                                Places like yours in your area usually range
                                from
                                $44 to $73
                            </div>

                        </div>
                        <div class="bg-secondary-subtle rounded-4 secondcont  h-25 mt-4">
